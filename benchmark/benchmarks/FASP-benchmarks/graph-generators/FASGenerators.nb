Cell[BoxData[
 RowBox[{
  RowBox[{"verify", "[", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"res", ",", "names"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"names", "=", 
      RowBox[{"FileBaseName", "/@", 
       RowBox[{"FileNames", "[", "\"\<*.mfas\>\"", "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", "monvar", "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Monitor", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"res", "=", 
           RowBox[{"AssociationMap", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{"With", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"{", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"graph", "=", 
                   RowBox[{"readAdjList", "[", 
                    RowBox[{"#", "<>", "\"\<.al\>\""}], "]"}]}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"fas", "=", 
                   RowBox[{"readEdgeList", "[", 
                    RowBox[{"#", "<>", "\"\<.mfas\>\""}], "]"}]}]}], 
                 "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"monvar", "=", "#"}], ";", "\[IndentingNewLine]", 
                 RowBox[{"minimalFasQ", "[", 
                  RowBox[{"graph", ",", "fas"}], "]"}]}]}], 
               "\[IndentingNewLine]", "]"}], "&"}], ",", 
             "\[IndentingNewLine]", "names"}], "\[IndentingNewLine]", "]"}]}],
           ";"}], "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
         "monvar"}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"Not", "[", 
        RowBox[{"And", "@@", "res"}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Echo", "@", 
        RowBox[{"Select", "[", 
         RowBox[{"res", ",", "Not"}], "]"}]}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->
  "In[173]:=",ExpressionUUID->"ce6859b8-be70-4a20-af37-5a46d38c7eaa"],

