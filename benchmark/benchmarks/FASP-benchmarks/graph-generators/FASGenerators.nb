Cell[BoxData[
 RowBox[{
  RowBox[{"randomOrient3", "[", "g_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Graph", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"VertexList", "[", "g", "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Flatten", "@", 
     RowBox[{"Replace", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"EdgeList", "[", "g", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"DirectedEdge", "[", 
         RowBox[{"u_", ",", "v_"}], "]"}], "\[RuleDelayed]", 
        "\[IndentingNewLine]", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"DirectedEdge", "[", 
            RowBox[{"u", ",", "v"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"DirectedEdge", "[", 
            RowBox[{"v", ",", "u"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"DirectedEdge", "[", 
              RowBox[{"u", ",", "v"}], "]"}], ",", 
             RowBox[{"DirectedEdge", "[", 
              RowBox[{"v", ",", "u"}], "]"}]}], "}"}]}], 
          "\[IndentingNewLine]", "}"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", "1", "}"}]}], "\[IndentingNewLine]", "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationGroup->True,
 CellLabel->
  "In[168]:=",ExpressionUUID->"7f5870a6-fd9b-4cee-b201-40306715dda9"],

