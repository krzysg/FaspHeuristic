 CellLabel->"In[61]:=",ExpressionUUID->"7d21cdbc-4534-4321-bd4a-2371908bf3cf"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"kautz", "[", 
   RowBox[{"d_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"IGKautzGraph", "[", 
   RowBox[{"d", ",", 
    RowBox[{"n", "-", "1"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"theta", "[", 
   RowBox[{"i_", ",", "d_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"d", "^", "i"}], "+", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], "^", "i"}], " ", "d"}]}]}], "\n", 
 RowBox[{
  RowBox[{"conv", "[", 
   RowBox[{"d_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Sum", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"EulerPhi", "[", "i", "]"}], 
     RowBox[{"theta", "[", 
      RowBox[{
       RowBox[{"n", "/", "i"}], ",", "d"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", 
      RowBox[{"Divisors", "[", "n", "]"}]}], "}"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"fs", "::", "approx"}], "=", 
   "\"\<The returned result is an approximation.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fs", "[", 
   RowBox[{"d_", ",", "n_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Which", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "\[Equal]", "1"}], ",", "\[IndentingNewLine]", "d", ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"2", "\[LessEqual]", "n", "\[LessEqual]", "7"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     FractionBox[
      RowBox[{"conv", "[", 
       RowBox[{"d", ",", "n"}], "]"}], "n"], "+", 
     FractionBox[
      RowBox[{"conv", "[", 
       RowBox[{"d", ",", 
        RowBox[{"n", "-", "1"}]}], "]"}], 
      RowBox[{"n", "-", "1"}]]}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"n", "\[GreaterEqual]", "8"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Message", "[", 
      RowBox[{"fs", "::", "approx"}], "]"}], ";", 
     RowBox[{
      FractionBox[
       SuperscriptBox["d", "n"], "n"], "+", 
      FractionBox[
       SuperscriptBox["d", 
        RowBox[{"n", "-", "1"}]], 
       RowBox[{"n", "-", "1"}]]}]}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", "True", ",", "\[IndentingNewLine]", "$Failed"}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellLabel->"In[62]:=",ExpressionUUID->"1c53d5a0-2210-4d75-970e-5b641f58345d"],

Cell[BoxData[
 RowBox[{
  RowBox[{"igKautzFASSize", "[", 
   RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
