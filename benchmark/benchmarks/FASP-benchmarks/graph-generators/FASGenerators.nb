Cell[BoxData[{
 RowBox[{
  RowBox[{"SeedRandom", "[", "123", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"cnt", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"graph", "=", 
        RowBox[{"SimpleGraph", "@", 
         RowBox[{"IGKautzGraph", "[", 
          RowBox[{"c", ",", "len"}], "]"}]}]}], ",", "name"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"name", "=", 
       RowBox[{"\"\<kautz-\>\"", "<>", 
        RowBox[{
         RowBox[{"int", "[", "4", "]"}], "[", 
         RowBox[{"cnt", "++"}], "]"}], "<>", "\"\<-\>\"", "<>", 
        RowBox[{"ToString", "[", "c", "]"}], "<>", "\"\<-\>\"", "<>", 
        RowBox[{"ToString", "[", "len", "]"}], "<>", "\"\<-\>\"", "<>", 
        RowBox[{"ToString", "@", 
         RowBox[{"VertexCount", "[", "graph", "]"}]}], "<>", "\"\<-\>\"", "<>", 
        RowBox[{"ToString", "@", 
         RowBox[{"EdgeCount", "[", "graph", "]"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"writeAdjList", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"name", "<>", "\"\<.al\>\""}], ",", "\[IndentingNewLine]", 
        "graph"}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"WriteString", "[", 
       RowBox[{
        RowBox[{"name", "<>", "\"\<.mfsize\>\""}], ",", 
        RowBox[{"igKautzFASSize", "[", 
         RowBox[{"c", ",", "len"}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Close", "[", 
       RowBox[{"name", "<>", "\"\<.mfsize\>\""}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"c", ",", "2", ",", "10"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"len", ",", "1", ",", "\[IndentingNewLine]", 
     RowBox[{"Which", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"c", "\[LessEqual]", "7"}], ",", "5", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"c", "\[LessEqual]", "8"}], ",", "4", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"c", "\[LessEqual]", "10"}], ",", "3", ",", 
       "\[IndentingNewLine]", "True", ",", "2"}], "\[IndentingNewLine]", 
      "]"}]}], "}"}]}], "\[IndentingNewLine]", "]"}]}], "Input",
 CellLabel->
  "In[74]:=",ExpressionUUID->"c5d6c119-0de7-463a-b2ea-f04087e4f8ab"]
}, Closed]],

