Cell[BoxData[{
 RowBox[{
  RowBox[{"SetSharedVariable", "[", "verifyParallel`monvar", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"verifyParallel", "[", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"res", ",", "names"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"names", "=", 
      RowBox[{"FileBaseName", "/@", 
       RowBox[{"FileNames", "[", "\"\<*.mfas\>\"", "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"verifyParallel`monvar", "=", "0"}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"dir", "=", 
         RowBox[{"Directory", "[", "]"}]}], "}"}], ",", 
       RowBox[{"ParallelEvaluate", "[", 
        RowBox[{"SetDirectory", "[", "dir", "]"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Monitor", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"res", "=", 
         RowBox[{"ParallelMap", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"With", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"{", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"graph", "=", 
                 RowBox[{"readAdjList", "[", 
                  RowBox[{"#", "<>", "\"\<.al\>\""}], "]"}]}], ",", 
                "\[IndentingNewLine]", 
                RowBox[{"fas", "=", 
                 RowBox[{"readEdgeList", "[", 
                  RowBox[{"#", "<>", "\"\<.mfas\>\""}], "]"}]}]}], 
               "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"verifyParallel`monvar", "++"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"minimalFasQ", "[", 
                RowBox[{"graph", ",", "fas"}], "]"}]}]}], 
             "\[IndentingNewLine]", "]"}], "&"}], ",", "\[IndentingNewLine]", 
           "names"}], "\[IndentingNewLine]", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"res", "=", 
         RowBox[{"AssociationThread", "[", 
          RowBox[{"names", ",", "res"}], "]"}]}], ";"}], 
       "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"verifyParallel`monvar", ",", 
         RowBox[{"ProgressIndicator", "[", 
          RowBox[{
           RowBox[{"Dynamic", "[", "verifyParallel`monvar", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", 
             RowBox[{
              RowBox[{"Length", "[", "names", "]"}], "+", "1"}]}], "}"}]}], 
          "]"}]}], "}"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"Not", "[", 
        RowBox[{"And", "@@", "res"}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Echo", "@", 
        RowBox[{"Select", "[", 
         RowBox[{"res", ",", "Not"}], "]"}]}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellLabel->"In[174]:=",ExpressionUUID->"e4d4db34-89f0-47d4-8b63-40144d92753b"]
